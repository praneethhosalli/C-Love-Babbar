#include <iostream>
using namespace std;

int main() {
  int n = 5;
  for(int i = 0; i < n; i++) {
   char ch;
   for(int j =0;j<i+1;j++){
    int number =j+1;
    ch = number +'A'-1;
    cout<<ch;
   }
   for (char alp =ch;alp>'A';){
    alp=alp-1;
    cout<<alp;
   }
    cout << endl;
  }
  return 0;
}
