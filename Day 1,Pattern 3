#include <iostream>
using namespace std;

int main() {
  for (int i=0;i<5;i++){
    for (int j=0;j<5;j++){
      if (i==0||i==4){
        cout<<"*";
      }
      else{
        if (j==0 || j==4){
          cout<<"*";
        }
        else {
          cout<<" ";
        }
      }
      
    }
    cout<<endl;
  }
  return 0;
}




#include <iostream>
using namespace std;

int main() {
  int r;
  cin>>r;
  int c ;
  cin>>c;
  
  for ( int i=0;i<r;i++){
    for ( int j=0;j<c;j++){
      if (i==0||i==r-1){
        cout<<"*";
      }
      else{
        if (j==0 || j==c-1){
          cout<<"*";
        }
        else {
          cout<<" ";
        }
      }
      
    }
    cout<<endl;
  }
  return 0;
}
