#include <iostream>
using namespace std;

void print(int arr[][3],int r,int c){
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            cout<<arr[i][j]<<" ";
        }
    }
}

int main(){
    int arr[3][3]={
        {1,2,3},{4,5,6},{7,8,9}
    };
    
    // cout<<arr[2][1];
    int r= 3;
    int c=3;
    print(arr,r,c);
    
    
    return 0;
}
