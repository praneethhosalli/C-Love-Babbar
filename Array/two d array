#include <iostream>
using namespace std;

void print(int arr[][3],int r,int c){
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            cout<<arr[i][j]<<" ";
        }
    }
}

int main(){
    int arr[3][3]={
        {1,2,3},{4,5,6},{7,8,9}
    };
    
    // cout<<arr[2][1];
    int r= 3;
    int c=3;
    print(arr,r,c);
    
    
    return 0;
}




#include <iostream>
using namespace std;

void print(int arr[][3],int r,int c){
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            cout<<arr[i][j]<<" ";
        }
        cout<<endl;
    }
}

int main(){
    // int arr[3][3]=
    // {{1,2,3},{4,5,6},{7,8,9}};
    
    int arr[3][3];
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            cin>>arr[i][j];
        }
    }
    int r= 3;
    int c=3;
    print(arr,r,c);
    
    
    return 0;
}






#include <iostream>
#include <climits>
using namespace std;

void print(int arr[][3],int r,int c){
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            cout<<arr[i][j]<<" ";
        }
        cout<<endl;
    }
}

bool find(int arr[][3],int r,int c,int t){
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            if(arr[i][j]==t){
                return true;
            }
        }
    }
    return false;
}

void max(int arr[][3],int r, int c){
    int max=INT_MIN;
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            if(arr[i][j]>max){
                max = arr[i][j];
            }
        }
    }
    cout<<"Max Number is "<<max;
}

int main(){
    // int arr[3][3]=
    // {{1,2,3},{4,5,6},{7,8,9}};
    
    int arr[3][3];
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            cin>>arr[i][j];
        }
    }
    int r= 3;
    int c=3;
    // print(arr,r,c);\
    
    // int t ;
    // cout<<"Enter the target"<<endl;
    // cin>>t;
    // bool q;
    // q=find(arr,r,c,t);
    
    max(arr,r,c);
    
    // cout<<q;
    return 0;
}



#include <iostream>
#include <climits>
using namespace std;

void print(int arr[][3],int r,int c){
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            cout<<arr[i][j]<<" ";
        }
        cout<<endl;
    }
}

bool find(int arr[][3],int r,int c,int t){
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            if(arr[i][j]==t){
                return true;
            }
        }
    }
    return false;
}

void max(int arr[][3],int r, int c){
    int max=INT_MIN;
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            if(arr[i][j]>max){
                max = arr[i][j];
            }
        }
    }
    cout<<"Max Number is "<<max;
}


void sum(int arr[][3],int r , int c){
    for(int i =0;i<r;i++){
        int sum = 0 ;
        for (int j=0;j<c;j++){
            sum += arr[i][j];
        }
        
        cout<<sum<<endl;
    }
    
}

int main(){
    // int arr[3][3]=
    // {{1,2,3},{4,5,6},{7,8,9}};
    
    int arr[3][3];
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            cin>>arr[i][j];
        }
    }
    int r= 3;
    int c=3;
    // print(arr,r,c);\
    
    // int t ;
    // cout<<"Enter the target"<<endl;
    // cin>>t;
    // bool q;
    // q=find(arr,r,c,t);
    
    // max(arr,r,c);
    sum(arr,r,c);
    
    // cout<<q;
    return 0;
}




#include <iostream>
#include <climits>
using namespace std;

void print(int arr[][3],int r,int c){
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            cout<<arr[i][j]<<" ";
        }
        cout<<endl;
    }
}

bool find(int arr[][3],int r,int c,int t){
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            if(arr[i][j]==t){
                return true;
            }
        }
    }
    return false;
}

void max(int arr[][3],int r, int c){
    int max=INT_MIN;
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            if(arr[i][j]>max){
                max = arr[i][j];
            }
        }
    }
    cout<<"Max Number is "<<max;
}


void sum(int arr[][3],int r , int c){
    for(int j =0;j<c;j++){
        int sum = 0 ;
        for(int i =0;i<r;i++){
            sum += arr[i][j];
        }
        
        cout<<sum<<endl;
    }
    
}

int main(){
    // int arr[3][3]=
    // {{1,2,3},{4,5,6},{7,8,9}};
    
    int arr[3][3];
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            cin>>arr[i][j];
        }
    }
    int r= 3;
    int c=3;
    // print(arr,r,c);\
    
    // int t ;
    // cout<<"Enter the target"<<endl;
    // cin>>t;
    // bool q;
    // q=find(arr,r,c,t);
    
    // max(arr,r,c);
    sum(arr,r,c);
    
    // cout<<q;
    return 0;
}



// 
// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    // std::cout << "Try programiz.pro";
    int arr[3][3]={{1,2,3},{4,5,6},{7,8,9}};
    int r=3;
    int c=3;
    int j = c-1;
    for(int i =0;i<r;i++){
        cout<<arr[i][j]<<endl;
        j--;
    }
    return 0;
}




// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    // std::cout << "Try programiz.pro";
    int arr[3][3]={{1,2,3},{4,5,6},{7,8,9}};
    int r=3;
    int c=3;
   
   for(int i = 0 ;i<r;i++){
       for(int j =i;j<c;j++){
           swap(arr[i][j],arr[j][i]);
       }
   }
   
    for(int i = 0 ;i<r;i++){
       for(int j =0;j<c;j++){
           cout<<arr[i][j]<<" ";
       }
       cout<<endl;
   }
   
    return 0;
}
