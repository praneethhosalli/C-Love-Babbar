#include <iostream>
using namespace std;

void print(int arr[][3],int r,int c){
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            cout<<arr[i][j]<<" ";
        }
    }
}

int main(){
    int arr[3][3]={
        {1,2,3},{4,5,6},{7,8,9}
    };
    
    // cout<<arr[2][1];
    int r= 3;
    int c=3;
    print(arr,r,c);
    
    
    return 0;
}




#include <iostream>
using namespace std;

void print(int arr[][3],int r,int c){
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            cout<<arr[i][j]<<" ";
        }
        cout<<endl;
    }
}

int main(){
    // int arr[3][3]=
    // {{1,2,3},{4,5,6},{7,8,9}};
    
    int arr[3][3];
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            cin>>arr[i][j];
        }
    }
    int r= 3;
    int c=3;
    print(arr,r,c);
    
    
    return 0;
}






#include <iostream>
#include <climits>
using namespace std;

void print(int arr[][3],int r,int c){
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            cout<<arr[i][j]<<" ";
        }
        cout<<endl;
    }
}

bool find(int arr[][3],int r,int c,int t){
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            if(arr[i][j]==t){
                return true;
            }
        }
    }
    return false;
}

void max(int arr[][3],int r, int c){
    int max=INT_MIN;
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            if(arr[i][j]>max){
                max = arr[i][j];
            }
        }
    }
    cout<<"Max Number is "<<max;
}

int main(){
    // int arr[3][3]=
    // {{1,2,3},{4,5,6},{7,8,9}};
    
    int arr[3][3];
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            cin>>arr[i][j];
        }
    }
    int r= 3;
    int c=3;
    // print(arr,r,c);\
    
    // int t ;
    // cout<<"Enter the target"<<endl;
    // cin>>t;
    // bool q;
    // q=find(arr,r,c,t);
    
    max(arr,r,c);
    
    // cout<<q;
    return 0;
}
