// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int uni(int arr[],int n){
    int ans= 0;
    for(int i=0;i<n;i++){
        ans = ans^arr[i];
    }
    return ans;
}

int main() {
    int arr[] = {10,1,2,3,4,4,3,2,1,};
    int n= 9;
    
    int final = uni(arr,n);
    cout<<final<<endl;
    

    return 0;
}
