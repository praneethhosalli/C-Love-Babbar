#include <iostream>
using namespace std;

int main() {
    // Write C++ code here
    int arr[10]={3,4,1,2,3};
    // cout<<arr[9];
    int n=10;
    for(int i=0;i<n;i++){
        cout<<arr[i]<<endl;
    }

    return 0;
}


#include <iostream>
using namespace std ;
int main() {
    int arr[5]={2,4,66,10,1000};
    int target =676;
    int n=5;
    bool flag=0;
    for(int i =0;i<n;i++){
        if(arr[i]==target){
            flag=1;
            break;
        }
    }
    if (flag==1){
        cout<<"Traget Found";
    }
    else{
        cout<<"Target Not Found";
    }
    return 0;
}


#include <iostream>
using namespace std ;
void print(int arr[],int size){
    for (int i=0;i<size;i++){
        cout<<arr[i]<<" ";
    }
    cout<<endl;
}
int main() {
    int arr[5]={10,2,3,4,5};
    int size =5;
    print(arr,size);
    return 0;
}
